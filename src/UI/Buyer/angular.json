{
  "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
  "version": 1,
  "newProjectRoot": "projects",
  "projects": {
    "default-components": {
      "projectType": "application",
      "schematics": {
        "@schematics/angular:component": {
          "style": "scss"
        }
      },
      "root": "",
      "sourceRoot": "src",
      "prefix": "app",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:browser",
          "options": {
            "outputPath": "dist/default-components",
            "index": "src/index.html",
            "main": "src/main.ts",
            "polyfills": "src/polyfills.ts",
            "tsConfig": "tsconfig.app.json",
            "aot": false,
            "preserveSymlinks": true,
            "assets": [
              "src/favicon.ico",
              "src/assets",
              "src/web.config"
            ],
            "styles": [
              "src/styles/main.scss"
            ],
            "scripts": []
          },
          "configurations": {
            "dev": {
              "stylePreprocessorOptions": {
                "includePaths": [
                  "src/styles/themes/waxing-the-city"
                ]
              },
              "optimization": false,
              "outputHashing": "all",
              "sourceMap": false,
              "extractCss": false,
              "namedChunks": false,
              "extractLicenses": true,
              "vendorChunk": false,
              "budgets": [ {
                  "type": "initial",
                  "maximumWarning": "2mb",
                  "maximumError": "6mb"
                },
                {
                  "type": "anyComponentStyle",
                  "maximumWarning": "6kb",
                  "maximumError": "10kb"
                }
              ]
            },
            "deploy": {
              "fileReplacements": [ {
                "replace": "src/environments/environment.local.ts",
                "with": "src/environments/environment.deploy.ts"
              } ],
              "styles": [
                {
                  "input": "src/styles/themes/anytime-fitness/_main.scss",
                  "bundleName": "anytimefitness",
                  "inject": false
                },
                {
                  "input": "src/styles/themes/bar-method/_main.scss",
                  "bundleName": "thebarmethod",
                  "inject": false
                },
                {
                  "input": "src/styles/themes/basecamp/_main.scss",
                  "bundleName": "basecampfitness",
                  "inject": false
                },
                {
                  "input": "src/styles/themes/brandweardesigns/_main.scss",
                  "bundleName": "brandweardesigns",
                  "inject": false
                },
                {
                  "input": "src/styles/themes/fastsigns/_main.scss",
                  "bundleName": "fastsigns",
                  "inject": false
                },
                {
                  "input": "src/styles/themes/go2partners/_main.scss",
                  "bundleName": "go2partners",
                  "inject": false
                },
                {
                  "input": "src/styles/themes/headstartdemo/_main.scss",
                  "bundleName": "headstartdemo",
                  "inject": false
                },
                {
                  "input": "src/styles/themes/waxing-the-city/_main.scss",
                  "bundleName": "waxingthecity",
                  "inject": false
                }
              ],
              "optimization": true,
              "outputHashing": "all",
              "sourceMap": false,
              "extractCss": true,
              "namedChunks": false,
              "extractLicenses": true,
              "vendorChunk": false,
              "budgets": [ {
                  "type": "initial",
                  "maximumWarning": "2mb",
                  "maximumError": "6mb"
                },
                {
                  "type": "anyComponentStyle",
                  "maximumWarning": "6kb",
                  "maximumError": "10kb"
                }
              ]
            }
          }
        },
        "serve": {
          "builder": "@angular-devkit/build-angular:dev-server",
          "options": {
            "browserTarget": "default-components:build",
            "sourceMap": {
              "scripts": true,
              "vendor": true
            }
          },
          "configurations": {
            "dev": {
              "browserTarget": "default-components:build:dev"
            },
            "deploy": {
              "browserTarget": "default-components:build:deploy"
            }
          }
        },
        "extract-i18n": {
          "builder": "@angular-devkit/build-angular:extract-i18n",
          "options": {
            "browserTarget": "default-components:build"
          }
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "main": "src/test.ts",
            "polyfills": "src/polyfills.ts",
            "tsConfig": "tsconfig.spec.json",
            "karmaConfig": "karma.conf.js",
            "assets": [
              "src/favicon.ico",
              "src/assets",
              "src/web.config"
            ],
            "styles": [
              "./node_modules/@angular/material/prebuilt-themes/indigo-pink.css",
              "src/styles/main.scss"
            ],
            "scripts": []
          }
        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": [
              "tsconfig.app.json",
              "tsconfig.spec.json",
              "e2e/tsconfig.json"
            ],
            "exclude": [
              "**/node_modules/**"
            ]
          }
        },
        "e2e": {
          "builder": "@angular-devkit/build-angular:protractor",
          "options": {
            "protractorConfig": "e2e/protractor.conf.js",
            "devServerTarget": "default-components:serve"
          },
          "configurations": {
            "test": {
              "devServerTarget": "default-components:serve:test"
            },
            "staging": {
              "devServerTarget": "default-components:serve:staging"
            }
          }
        }
      }
    }
  },
  "defaultProject": "default-components"
}