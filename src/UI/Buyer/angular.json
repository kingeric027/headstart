            "browserTarget": "default-components:build",
            "sourceMap": {
              "scripts": true,
              "vendor": true
            }
          },
          "configurations": {
            "dev": {
              "browserTarget": "default-components:build:dev"
            },
            "anytimefitness-production": {
              "browserTarget": "default-components:build:anytimefitness-production"
            },
            "anytimefitness-staging": {
              "browserTarget": "default-components:build:anytimefitness-staging"
            },
            "basecampfitness-production": {
              "browserTarget": "default-components:build:basecampfitness-production"
            },
            "basecampfitness-staging": {
              "browserTarget": "default-components:build:basecampfitness-staging"
            },
            "waxingthecity-production": {
